{
  "name": "spradling-webpage",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack -p 3005",
    "dev-es": "next dev --turbopack -p 3006",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "fetch-schema": "curl -X POST https://api.dev.spradling.group/graphql/ -H \"Content-Type: application/json\" -d \"{\\\"query\\\":\\\"query IntrospectionQuery { __schema { queryType { name } mutationType { name } types { ...FullType } directives { name description locations args { ...InputValue } } } } fragment FullType on __Type { kind name description fields(includeDeprecated: true) { name description args { ...InputValue } type { ...TypeRef } isDeprecated deprecationReason } inputFields { ...InputValue } interfaces { ...TypeRef } enumValues(includeDeprecated: true) { name description isDeprecated deprecationReason } possibleTypes { ...TypeRef } } fragment InputValue on __InputValue { name description type { ...TypeRef } defaultValue } fragment TypeRef on __Type { kind name ofType { kind name ofType { kind name ofType { kind name } } } }\\\"}\" > src/graphql/schema.json",
    "prepare-schema": "node src/scripts/clean-schema.mjs",
    "update-graphql": "npm run fetch-schema",
    "codegen": "npm run update-graphql && npm run prepare-schema && graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/client-integration-nextjs": "^0.12.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.1",
    "@graphql-tools/code-file-loader": "^8.1.20",
    "@graphql-tools/graphql-file-loader": "^8.0.20",
    "@graphql-tools/json-file-loader": "^8.0.18",
    "@graphql-tools/load": "^8.1.0",
    "@graphql-tools/utils": "^10.8.6",
    "@hookform/resolvers": "^5.0.1",
    "@t3-oss/env-nextjs": "^0.13.4",
    "@u-wave/react-vimeo": "^0.9.12",
    "@vimeo/player": "^2.30.1",
    "@vis.gl/react-google-maps": "^1.5.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.11.0",
    "graphql-codegen": "^0.4.0",
    "next": "^15.3.2",
    "next-intl": "^4.0.2",
    "react": "^19.0.0",
    "react-country-state-city": "^1.1.12",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "react-international-phone": "^4.5.0",
    "react-loading-skeleton": "^3.5.0",
    "react-phone-input-2": "^2.15.1",
    "react-player": "^2.16.0",
    "sonner": "^2.0.5",
    "swiper": "^12.0.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "graphql-cli": "^4.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
